<idea-plugin>
    <id>oodmi.log-idea-plugin</id>
    <name>Log helper for java</name>
    <vendor email="dmitry.o.ovchinnikov@gmail.com" url="http://www.oodmi.ru">oodmi</vendor>

    <description><![CDATA[
    This plugin helps you to find suitable logger.<br>
    ]]></description>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.template.postfixTemplateProvider language="JAVA"
                                                      implementationClass="org.jetbrains.logger.LogTemplateProvider"/>
    </extensions>

    <actions>
    </actions>
</idea-plugin>